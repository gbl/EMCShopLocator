drop table if exists signs;
create table signs (
	server varchar(30),
	resnumber varchar(30),
	x integer,
	y integer,
	z integer,
	amount integer,
	buy integer,
	sell integer,
	owner varchar(30),
	item varchar(60));
create index signs_i1 on signs (server, x, z, y);
create index signs_i2 on signs (server, resnumber);
